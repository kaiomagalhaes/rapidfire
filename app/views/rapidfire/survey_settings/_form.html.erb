<%= form_for survey_setting do |f| %>
  <%- if survey_setting.errors.any? %>
    <ul>
      <%- survey_setting.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  <% end %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit %>
<% end %>
